<div class="container">
  <h1>Booking "<%= @book.title %>"</h1>
  <%= simple_form_for([@book, @booking]) do |f| %>
    <%= f.input :start_date,
              as: :string,
              input_html: { data: { controller: "datepicker" } } %>
    <%= f.input :end_date,
              as: :string,
              input_html: { data: { controller: "datepicker" } } %>
     <%= f.submit "Submit" %>
  <% end %>
</div>
